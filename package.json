{
  "name": "@slovakia-kosice/idsk-react",
  "version": "4.1.1",
  "description": "React native components for IDSK 3.0",
  "type": "module",
  "scripts": {
    "clean-dist": "rm -rf ./dist",
    "rollup": "npm run clean-dist && npm run build-styles-prod && rollup -c",
    "prettier": "prettier --write src",
    "lint": "npx tsc --noEmit && npx eslint src",
    "test": "jest",
    "build-styles-dev": "tailwindcss -i ./src/styles/index.css -o ./src/styles/idsk3_theme.css --watch",
    "build-styles-prod": "tailwindcss -i ./src/styles/index.css -o ./src/styles/idsk3_theme.css -m",
    "storybook": "storybook dev -p 6006",
    "dev": "concurrently \"npm:build-styles-dev\" \"npm:storybook\"",
    "build-storybook": "storybook build",
    "build-svg-icons": "npx @svgr/cli --out-dir src/svgIcons --icon --index-template svgr-index-template.js --replace-attr-values '#000=currentColor' -- node_modules/@slovakia-kosice/idsk-core/dist/assets/svgIcons",
    "build-svg-images": "npx @svgr/cli --out-dir src/svgImages --no-dimensions --replace-attr-values '#000=currentColor' -- node_modules/@slovakia-kosice/idsk-core/dist/assets/svgImages",
    "deploy": "npm run rollup && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.slovenskoit.sk:components/shared-web-libs/esvk_web_lib_idsk_react.git"
  },
  "author": "Slovensko IT",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://gitlab.slovenskoit.sk/components/shared-web-libs/esvk_web_lib_idsk_react/issues"
  },
  "homepage": "https://github.com/Mesto-Kosice/web_lib_idsk3_react#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/js": "^9.19.0",
    "@headlessui/react": "^2.2.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@slovakia-kosice/idsk-core": "^3.1.9",
    "@storybook/addon-essentials": "^8.5.2",
    "@storybook/addon-interactions": "^8.5.2",
    "@storybook/addon-onboarding": "^8.5.2",
    "@storybook/addons": "^7.6.17",
    "@storybook/blocks": "^8.5.2",
    "@storybook/builder-vite": "^8.5.2",
    "@storybook/react": "^8.5.2",
    "@storybook/react-vite": "^8.5.2",
    "@storybook/test": "^8.5.2",
    "@svgr/cli": "^8.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.10",
    "@types/react": "^19.0.8",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "rollup": "^4.32.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "storybook": "^8.5.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0"
  },
  "peerDependencies": {
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "react": "^19.0.0",
    "react-datepicker": "^7.6.0",
    "react-dropzone": "^14.3.5",
    "react-focus-lock": "^2.13.5",
    "rifm": "^0.12.1",
    "tailwind-merge": "^2.6.0",
    "uuid": "^11.0.5"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "types": "dist/index.d.ts"
}
