{
  "name": "@eslovensko/idsk-react",
  "version": "3.2.30",
  "description": "React native components for IDSK 3.0",
  "scripts": {
    "clean-dist": "rm -rf ./dist",
    "rollup": "npm run clean-dist && npm run build-styles-prod && rollup -c",
    "prettier": "prettier --write src",
    "lint": "npx tsc --noEmit && npx eslint src",
    "test": "jest",
    "build-styles-dev": "tailwindcss -i ./src/styles/index.css -o ./src/styles/idsk3_theme.css --watch",
    "build-styles-prod": "tailwindcss -i ./src/styles/index.css -o ./src/styles/idsk3_theme.css -m",
    "storybook": "concurrently \"npm run build-styles-dev\" \"start-storybook -p 6006\"",
    "build-storybook": "npm run build-styles-prod && build-storybook",
    "build-svg-icons": "npx @svgr/cli --out-dir src/svgIcons --icon --index-template svgr-index-template.js --replace-attr-values '#000=currentColor' -- node_modules/@eslovensko/idsk-core/dist/assets/svgIcons",
    "build-svg-images": "npx @svgr/cli --out-dir src/svgImages --no-dimensions --replace-attr-values '#000=currentColor' -- node_modules/@eslovensko/idsk-core/dist/assets/svgImages",
    "deploy": "npm run rollup && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.slovenskoit.sk:components/shared-web-libs/esvk_web_lib_idsk_react.git"
  },
  "author": "Slovensko IT",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://gitlab.slovenskoit.sk/components/shared-web-libs/esvk_web_lib_idsk_react/issues"
  },
  "homepage": "https://gitlab.slovenskoit.sk/components/shared-web-libs/esvk_web_lib_idsk_react#readme",
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@eslovensko/idsk-core": "^3.0.25",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-interactions": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/addons": "^6.5.6",
    "@storybook/builder-webpack5": "^6.5.6",
    "@storybook/manager-webpack5": "^6.5.6",
    "@storybook/react": "^6.5.6",
    "@storybook/testing-library": "^0.0.11",
    "@storybook/theming": "^6.5.6",
    "@svgr/cli": "^6.2.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.40",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "autoprefixer": "^10.4.4",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.0.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.5.1",
    "postcss": "^8.4.12",
    "prettier": "^2.6.0",
    "rollup": "^2.70.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2"
  },
  "peerDependencies": {
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-focus-lock": "^2.9.1",
    "uuid": "^8.3.2"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "types": "dist/index.d.ts"
}
